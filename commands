docker run -d -e POSTGRES_USER=docker \ 
    -e POSTGRES_PASSWORD=docker \ 
    -e POSTGRES_DB=docker \
     --name dockerdb  postgres:9-alpine


docker run -it --link dockerdb governmentpaas/psql \ 
    psql -U docker -h dockerdb






